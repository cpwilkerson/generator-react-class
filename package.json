{
  "name": "generator-react-class",
  "version": "2.3.1",
  "description":
    "Creates a react class js file, a test js file, and a sass file.",
  "homepage": "",
  "author": {
    "name": "Christian Wilkerson",
    "email": "cpwilkerson72@gmail.com",
    "url": ""
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["yeoman generator", "react", "", "yeoman-generator"],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "merge": "^1.2.1",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "cpwilkerson/generator-react-class",
  "license": "MIT"
}
